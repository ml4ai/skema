#!/bin/bash

eval "$(conda shell.bash hook)"
conda deactivate
conda activate image2mathml_venv

python3 create_dataset.py

python3 sampling_dataset/preprocess_latex.py training_data/sample_data/original_latex.lst training_data/sample_data/latex.norm.lst

# zip everyting intraining_data/sample_data to data.tar.gz
cd training_data/sample_data/
tar -czvf data.tar.gz sample_data
mv data.tar.gz ../
cd ../

echo "Done"